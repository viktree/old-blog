{
  "name": "viktree-personal-blog",
  "private": true,
  "description": "My personal blog",
  "version": "0.1.0",
  "author": "Vikram Venkataramanan <vikram.venkataramanan@mail.utoronto.ca>",
  "dependencies": {
    "disqus-react": "^1.0.10",
    "gatsby": "^2.24.83",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-feed": "^2.7.0",
    "gatsby-plugin-manifest": "^2.4.32",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-sharp": "^2.6.37",
    "gatsby-plugin-sitemap": "^2.4.14",
    "gatsby-plugin-typography": "^2.6.0",
    "gatsby-remark-copy-linked-files": "^2.4.0",
    "gatsby-remark-highlight-code": "^2.1.1",
    "gatsby-remark-images": "^3.5.0",
    "gatsby-remark-katex": "^3.4.0",
    "gatsby-remark-prismjs": "^3.5.14",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-remark-smartypants": "^2.4.0",
    "gatsby-source-filesystem": "^2.3.31",
    "gatsby-transformer-remark": "^2.10.0",
    "gatsby-transformer-sharp": "^2.6.0",
    "katex": "^0.12.0",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-typography": "^0.16.19",
    "typeface-bitter": "0.0.71",
    "typeface-fira-code": "^1.1.4",
    "typeface-pt-sans": "0.0.72",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.3.0",
    "prettier": "2.1.1",
    "pretty-quick": "^3.0.2",
    "snyk": "^1.401.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,css,ts,tsx,json,md}\"",
    "lint": "eslint --ext .js,.jsx src",
    "prepare": "yarn run snyk-protect",
    "serve": "gatsby serve",
    "snyk-protect": "snyk protect",
    "start": "gatsby develop",
    "test": "snyk test && echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "[ ! -e .nvmrc ] && node -v  > .nvmrc || : && git add .nvmrc && pretty-quick --staged && eslint --ext .js,.jsx src"
    }
  },
  "snyk": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
